<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/openByEditor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="OUVRIR PAR EDITEUR"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/openByCountry" />

    <Button
        android:id="@+id/openCategory"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="OUVRIR PAR CATEGORIE"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="246dp" />

    <Button
        android:id="@+id/openByCountry"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="OUVRIR PAR PAYS"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="351dp" />


</androidx.constraintlayout.widget.ConstraintLayout>